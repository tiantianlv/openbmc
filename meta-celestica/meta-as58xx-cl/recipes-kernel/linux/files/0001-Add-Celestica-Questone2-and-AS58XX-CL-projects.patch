+#if defined(CONFIG_AS58XX_CL)
+static void led_delay_worker(struct work_struct *work)
+{
+	int speed, val;
+	struct ftgmac100 *lp = container_of(work, struct ftgmac100, led_dwq);
+	struct workqueue_struct *wq = lp->led_wq;
+
+
+	val = ftgmac100_mdiobus_read(lp->mii_bus, 0x0e, 0x19); //read link speed
+	if(val < 0) {
+		return;
+	}
+	val = (val >> 8) & 0x07;
+	switch(val) {
+		case 0x1:
+		case 0x2:
+			speed = 10;
+			ftgmac100_mdiobus_write(lp->mii_bus, 0x0e, 0x1c, 0xb4fe);//10M LED
+			break;
+		case 0x3:
+		case 0x4:
+		case 0x5:
+			speed = 100;
+			ftgmac100_mdiobus_write(lp->mii_bus, 0x0e, 0x1c, 0xb4fe);//100M LED
+			break;
+		case 0x6:
+		case 0x7:
+			speed = 1000;
+			ftgmac100_mdiobus_write(lp->mii_bus, 0x0e, 0x1c, 0xb4ef);//1000M LED
+			break;
+		default:
+			speed = 0;
+			ftgmac100_mdiobus_write(lp->mii_bus, 0x0e, 0x1c, 0xb4ee);//No link
+			break;
+	}
+
+	queue_delayed_work(wq, work, HZ);
+	return;
+}
+#endif
